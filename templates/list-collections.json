{
  "sections": {
    "collection-list-header": {
      "type": "custom-section",
      "blocks": {
        "collection-list-heading": {
          "type": "heading",
          "settings": {
            "heading": "Collections",
            "heading_size": "h1",
            "alignment": "start",
            "size": {
            },
            "spacing": {
            }
          }
        }
      },
      "block_order": [
        "collection-list-heading"
      ],
      "settings": {
        "spacing": {
          "padding-block-end": "0px"
        },
        "layout": {
          "justify-content": "flex-start"
        }
      }
    },
    "main-collection": {
      "type": "main-list-collections",
      "blocks": {
        "collection-card": {
          "type": "collection-card",
          "static": true,
          "settings": {
            "product": "{{ context.collection }}"
          },
          "blocks": {
            "collection-image": {
              "type": "image",
              "settings": {
                "image": "{{ context.collection.image }}",
                "media_featured": true
              }
            },
            "collection-title": {
              "type": "collection-title",
              "settings": {
                "product": "{{ context.collection }}",
                "heading_size": "h4"
              }
            }
          },
          "block_order": [
            "collection-image",
            "collection-title"
          ]
        }
      },
      "settings": {
      }
    }
  },
  "order": [
    "collection-list-header",
    "main-collection"
  ]
}
